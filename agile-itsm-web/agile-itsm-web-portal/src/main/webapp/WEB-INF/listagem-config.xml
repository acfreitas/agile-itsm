    <listagem tipo='SQL' nome='PROC_GINEC' descricao='Procedimentos ginecológicos'>
        <SQL>
                SELECT DISTINCT p.idprocedimento AS idprocedimento, nomeprocedimento
                FROM procedimentos p, procedimentoespecialidade pe, especialidades e
                WHERE pe.idprocedimento = p.idprocedimento
                AND upper (e.nome) = 'GINECOLOGIA'
                AND e.idespecialidade = pe.idespecialidade
                AND INSTR ((SELECT ',' || TO_CHAR (valor) FROM parametros
                WHERE modulo = 'EXAMES' AND nomeparametro = 'EXAMES_CODIGOS_ULTRASONOGRAFIA' AND idempresa = 1),',' || TO_CHAR (p.idprocedimento) || ',',1) = 0
                ORDER BY 2 ASC
        </SQL>
        <campos>
            <campo nome='idProcedimento'/>
            <campo nome='nomeProcedimento' descricao='Procedimento'/>
        </campos>
    </listagem>
    <listagem tipo='SQL' nome='PROC_GINEC_IMAGEM' descricao='Procedimentos ginecológicos - Imagens'>
        <SQL>
                SELECT DISTINCT p.idprocedimento AS idprocedimento, nomeprocedimento
                FROM procedimentos p, procedimentoespecialidade pe, especialidades e
                WHERE pe.idprocedimento = p.idprocedimento
                AND upper (e.nome) = 'GINECOLOGIA'
                AND e.idespecialidade = pe.idespecialidade
                AND INSTR ((SELECT ',' || TO_CHAR (valor) FROM parametros
                WHERE modulo = 'EXAMES' AND nomeparametro = 'EXAMES_CODIGOS_ULTRASONOGRAFIA' AND idempresa = 1),',' || TO_CHAR (p.idprocedimento) || ',',1) != 0
                ORDER BY 2 ASC
        </SQL>
        <campos>
            <campo nome='idProcedimento'/>
            <campo nome='nomeProcedimento' descricao='Procedimento'/>
        </campos>
    </listagem>
